{
	"name": "bymr-launcher-frontend",
	"version": "0.3.0",
	"description": "A Svelte frontend application for the Backyard Monsters Refitted Launcher",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"tauri": "tauri",
		"desktop:dev": "npm run tauri dev",
		"desktop:build": "npm run tauri build",
		"android:dev": "npx @tauri-apps/cli android dev",
		"android:build": "npm run tauri android build && powershell -ExecutionPolicy Bypass -File ./scripts/sign-android-apk.ps1",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
		"@tauri-apps/api": "2.8.0",
		"@tauri-apps/plugin-http": "2.5.2",
		"@tauri-apps/plugin-os": "^2.3.1",
		"@tauri-apps/plugin-process": "2.3.0",
		"@tauri-apps/plugin-updater": "2.9.0",
		"bits-ui": "0.22.0",
		"clsx": "2.1.1",
		"tailwind-merge": "3.2.0",
		"tailwind-variants": "1.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "6.0.0",
		"@sveltejs/adapter-static": "3.0.8",
		"@sveltejs/kit": "2.20.7",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@tauri-apps/cli": "2.8.4",
		"autoprefixer": "10.4.21",
		"phosphor-svelte": "3.0.1",
		"postcss": "8.5.3",
		"svelte": "5.28.2",
		"tailwindcss": "3.4.17",
		"tslib": "2.8.1",
		"typescript": "5.8.3",
		"vite": "6.3.3"
	}
}
